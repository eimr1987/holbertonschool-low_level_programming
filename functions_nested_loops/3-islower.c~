#include "main.h"


/**
 * _islower - funcion que nos dice si es minuscula
 * @c: esto me quitÃ³ tiempo
 * Return: 1 minuscula, 0 en caso contrario
 *
 */
int _islower(int c)
{
	/*Mediante ascii sabremos si es minuscula*/
	if (c >= 97 && c <= 122)
	{
		return (1); /* si es minuscula*/
	}
	return (0); /*no es minuscula*/
}
