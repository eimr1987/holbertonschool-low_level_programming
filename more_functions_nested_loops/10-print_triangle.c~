#include "main.h"
1;95;0c
/**
 * print_triangle - imprime gatos, que forman triangulos
 *
 * @size: validacion
 *
 * Return: cero
 *
 */
void print_triangle(int size)
{
	int i, j, k;

	if (size <= 0)
	{
		_putchar('\n');
	}
	else
	{
		for (i = 0; i < size; i++)
		{
			for (y = size - i; j > 1; y--)
			{
				_putchar(' ');
			}
			for (k = 0; k <= i; k++)
			{
				_putchar('#');
			}
			_putchar('\n');
		}
	}
}
